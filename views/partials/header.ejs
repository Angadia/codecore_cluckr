<header class="header">
    <div class="branding">
      <img src="assets/cluckr_logo.jpg" alt="Cluckr Logo" class="logo">
    </div>
    <nav class="nav">
      <ul class="menu">
        <li><button class="btn_header_cluck">Cluck!</button></li>
        
        <% if (loggedInUsername) { %>

          <li><label class="lbl_header_username"><%= loggedInUsername %></label></li>
          <li>
            <form action="/sign_out" method="POST">
              <input type="submit" value="Sign Out">
            </form>
          </li>
          
        <% } else { %>
 
          <li>
            <form action="/sign_in" method="GET">
              <input type="submit" value="Sign In">
            </form>
          </li>
        <% } %>
        
      </ul>
    </nav>
</header>